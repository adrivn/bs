---
permalink: sf.xml
eleventyExcludeFromCollections: true
---
<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
<channel>
	<atom:link href="{{ site.stationsfeed.path | absoluteUrl(site.url) }}" rel="self" type="application/rss+xml" />
	<title>{{ meta.title }}</title>
	<description>{{ meta.description }}</description>
	<link>{{ meta.url }}</link>
	<pubDate>{{ page.date | dateToRSS }}</pubDate>
	<webMaster>{{ meta.email }} ({{ meta.author }})</webMaster>
	{%- for setup in newstations %}
	{% set absolutePostUrl %}{{ setup.id | absoluteUrl(meta.url+"/setup/") }}{% endset %}
	<item>
		<title>{{ setup.title }}</title>
		<link>{{ absolutePostUrl }}</link>
		<description>Setup submitted by {{ setup.author }} in the /r/battlestations subreddit</description>
		<enclosure url="{{ setup.img }}" type="image/jpeg" length="0"/>
		<pubDate>{{ setup.date | dateISOtoRSS }}</pubDate>
		<guid>{{ absolutePostUrl }}</guid>
	</item>
	{%- endfor %}
</channel>
</rss>
